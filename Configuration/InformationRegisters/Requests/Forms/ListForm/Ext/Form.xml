	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	InformationRegisterRequests.JobNumber			AS JobNumber,
	InformationRegisterRequests.PendingDate			AS PendingDate,
	InformationRegisterRequests.RequestDate			AS RequestDate,
	InformationRegisterRequests.RequestDateInMS		AS RequestDateInMS,
	
	DATEDIFF(InformationRegisterRequests.PendingDate, InformationRegisterRequests.RequestDate, SECOND)
												AS SecondsInPending
												
FROM
	InformationRegister.Requests AS InformationRegisterRequests</QueryText>
				<Field xsi:type="dcssch:DataSetFieldField">
					<dcssch:dataPath>PendingDate</dcssch:dataPath>
					<dcssch:field>PendingDate</dcssch:field>
