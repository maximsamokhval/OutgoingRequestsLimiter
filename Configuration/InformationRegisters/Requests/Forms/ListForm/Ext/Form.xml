		<Attribute name="Exceedings" id="3">
			<Title>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Exceedings</v8:content>
				</v8:item>
			</Title>
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>SELECT
	Requests.RequestDate AS RequestDate,
	SUM(1) AS RequestsNumber
FROM
	InformationRegister.Requests AS Requests

GROUP BY
	Requests.RequestDate        
	
HAVING 
	SUM(1) &gt; &amp;MaximumRequestsNumber</QueryText>
				<Field xsi:type="dcssch:DataSetFieldField">
					<dcssch:dataPath>RequestDate</dcssch:dataPath>
					<dcssch:field>RequestDate</dcssch:field>
				</Field>
				<Field xsi:type="dcssch:DataSetFieldField">
					<dcssch:dataPath>RequestsNumber</dcssch:dataPath>
